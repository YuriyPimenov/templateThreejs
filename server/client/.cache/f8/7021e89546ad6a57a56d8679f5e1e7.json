ne":119,"column":11},"source":"scripts/3DComponents/managerObjects.js","original":{"line":57,"column":15}},{"name":"animate","generated":{"line":119,"column":12},"source":"scripts/3DComponents/managerObjects.js","original":{"line":57,"column":16}},{"generated":{"line":119,"column":19},"source":"scripts/3DComponents/managerObjects.js","original":{"line":57,"column":12}},{"name":"mesh","generated":{"line":119,"column":20},"source":"scripts/3DComponents/managerObjects.js","original":{"line":57,"column":24}},{"generated":{"line":119,"column":24},"source":"scripts/3DComponents/managerObjects.js","original":{"line":57,"column":12}},{"generated":{"line":120,"column":0},"source":"scripts/3DComponents/managerObjects.js","original":{"line":58,"column":9}},{"generated":{"line":121,"column":0},"source":"scripts/3DComponents/managerObjects.js","original":{"line":59,"column":5}}],"sources":{"scripts/3DComponents/managerObjects.js":"import { BoxBufferGeometry, MeshBasicMaterial, MeshStandardMaterial, Mesh } from 'three'\nexport default class ManagerObjects{\n    constructor(){\n        this.objects = [\n            {\n                type: 'mesh',\n                constructor: 'BoxBufferGeometry',\n                name: 'helloCube',\n                sizes: {\n                    x: 2,\n                    y: 2,\n                    z: 2\n                },\n                material:{\n                    //Объект спецификации\n                    specObject:{\n                        color: 0x800080\n                    }\n                },\n                animate: (mesh)=>{\n                    mesh.rotation.z += 0.01;\n                    mesh.rotation.x += 0.01;\n                    mesh.rotation.y += 0.01;\n                }\n            }\n        ];\n        this.meshes = [];\n    }\n\n    createObjects(){\n        for(let object of this.objects){\n            switch (object.constructor) {\n                case 'BoxBufferGeometry':\n                    let geometry = new BoxBufferGeometry( object.sizes.x, object.sizes.y, object.sizes.z );\n                    geometry.name = object.name;\n                    let material = new MeshStandardMaterial(object.material.specObject);\n                    // material.color.set( '0xff0000' )\n                    let mesh = new Mesh( geometry, material );\n                    this.meshes.push(mesh);\n                    break;\n                default:\n                    console.log('Нет такого конструктора');\n            }\n        }\n    }\n    addObjects(scene){\n        for(let mesh of this.meshes){\n            scene.add( mesh );\n        }\n    }\n\n    animate(){\n\n        for(let i=0; i<this.meshes.length;i++){\n            let mesh = this.meshes[i];\n            let obj = this.objects[i];\n            obj.animate(mesh);\n        }\n    }\n}\n"},"lineCount":null}},"error":null,"hash":"14b66b13533729f2e3850c58c85dfa41","cacheData":{"env":{}}}